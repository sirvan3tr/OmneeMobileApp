{
  "presets": [
    "@babel/env",
    "react-native",
    "@babel/preset-flow"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/plugin-proposal-optional-chaining"],
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@assets": "./assets",
          "@common": "./src/common",
          "@components": "./src/components"
        }
      }
    ]
  ]
}
